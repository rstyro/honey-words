/*flex*/
.col-box{
display:-webkit-box;
display:-ms-flexbox;
display:flex;display:-webkit-flex;
-webkit-box-orient:vertical;
-webkit-box-direction:normal;
    -ms-flex-direction:column;
        flex-direction:column;-webkit-flex-direction:column;
-ms-flex-wrap:wrap;
    flex-wrap:wrap;-webkit-flex-wrap:wrap;
height:100%;overflow: hidden;
}
.row-box{
display:-webkit-box;
display:-ms-flexbox;
display:flex;display:-webkit-flex;
-webkit-box-orient:vertical;
-webkit-box-direction:normal;
    -ms-flex-direction:wrap;
        flex-direction:wrap;-webkit-flex-direction:wrap;
-ms-flex-wrap:wrap;
    flex-wrap:wrap;-webkit-flex-wrap:wrap;
width:100%;overflow: hidden;
}
.flex1{
  -webkit-box-flex:1;
      -ms-flex-positive:1;
          flex-grow:1;-webkit-flex-grow:1;
  -ms-flex-negative:1;
      flex-shrink:1;-webkit-flex-shrink:1;
  -ms-flex-preferred-size:0px;
      flex-basis:0px;-webkit-flex-basis:0px;
}
.text-none{overflow: hidden; -o-text-overflow: ellipsis; text-overflow: ellipsis; white-space: nowrap;}
.clearfix{clear: both;}


